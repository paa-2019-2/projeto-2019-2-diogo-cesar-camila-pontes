weirdlength : THEORY
BEGIN

carcdrcdr_length : LEMMA
		 FORALL(l:list[nat] | NOT null?(l) AND NOT null?(cdr(l))):
		 length(l) > length(cons(car(l), cdr(cdr(l))))


END weirdlength
